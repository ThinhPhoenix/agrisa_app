{
  "expo": {
    "name": "arisa_app",
    "slug": "arisa_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic_agrisa_logo.png",
    "scheme": "arisaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_agrisa_logo.png",
        "backgroundColor": "#fbe9a8"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ic_agrisa_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ic_agrisa_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fbe9a8"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    }
  }
}
