{
    "cli": {
        "version": ">= 16.19.3",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_URL": "$EXPO_PUBLIC_API_URL",
                "EXPO_PUBLIC_GEMINI_KEY": "$EXPO_PUBLIC_GEMINI_KEY",
                "EXPO_PUBLIC_OPENMAPVN_KEY": "$EXPO_PUBLIC_OPENMAPVN_KEY",
                "EXPO_PUBLIC_VN2000_API_URL": "$EXPO_PUBLIC_VN2000_API_URL"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_URL": "$EXPO_PUBLIC_API_URL",
                "EXPO_PUBLIC_GEMINI_KEY": "$EXPO_PUBLIC_GEMINI_KEY",
                "EXPO_PUBLIC_OPENMAPVN_KEY": "$EXPO_PUBLIC_OPENMAPVN_KEY",
                "EXPO_PUBLIC_VN2000_API_URL": "$EXPO_PUBLIC_VN2000_API_URL"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_API_URL": "$EXPO_PUBLIC_API_URL",
                "EXPO_PUBLIC_GEMINI_KEY": "$EXPO_PUBLIC_GEMINI_KEY",
                "EXPO_PUBLIC_OPENMAPVN_KEY": "$EXPO_PUBLIC_OPENMAPVN_KEY",
                "EXPO_PUBLIC_VN2000_API_URL": "$EXPO_PUBLIC_VN2000_API_URL"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
